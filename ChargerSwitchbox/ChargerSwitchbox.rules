
(rules PCB ChargerSwitchbox
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 145)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 600.2 (type default_ChargerCurrent))
    (clear 300.2 (type default_PowerCurrent))
    (clear 50.0 (type smd_smd))
    (clear 600.2 (type smd_ChargerCurrent))
    (clear 300.2 (type smd_PowerCurrent))
    (clear 600.2 (type "kicad_default"_ChargerCurrent))
    (clear 300.2 (type "kicad_default"_PowerCurrent))
    (clear 600.2 (type ChargerCurrent_ChargerCurrent))
    (clear 600.2 (type ChargerCurrent_PowerCurrent))
    (clear 300.2 (type PowerCurrent_PowerCurrent))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_6500:5000_um"
    (shape
      (circle F.Cu 6500.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 6500.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1200:600_um"
    (shape
      (circle F.Cu 1200.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1200.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_6500:5000_um" "Via[0-1]_6500:5000_um" default
  )
  (via 
    "Via[0-1]_1200:600_um" "Via[0-1]_1200:600_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_6500:5000_um-kicad_default" "Via[0-1]_6500:5000_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1200:600_um-kicad_default" "Via[0-1]_1200:600_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-ChargerCurrent" "Via[0-1]_800:400_um" ChargerCurrent
  )
  (via 
    "Via[0-1]_6500:5000_um-ChargerCurrent" "Via[0-1]_6500:5000_um" ChargerCurrent
  )
  (via 
    "Via[0-1]_1200:600_um-ChargerCurrent" "Via[0-1]_1200:600_um" ChargerCurrent
  )
  (via 
    "Via[0-1]_800:400_um-PowerCurrent" "Via[0-1]_800:400_um" PowerCurrent
  )
  (via 
    "Via[0-1]_6500:5000_um-PowerCurrent" "Via[0-1]_6500:5000_um" PowerCurrent
  )
  (via 
    "Via[0-1]_1200:600_um-PowerCurrent" "Via[0-1]_1200:600_um" PowerCurrent
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    ChargerCurrent "Via[0-1]_6500:5000_um-ChargerCurrent"
  )
  (via_rule
    PowerCurrent "Via[0-1]_1200:600_um-PowerCurrent"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "/AIRS_FINAL_CHARGER" "/CHARGER_CONTROL" "Net-(R1-Pad1)" "/CHARGER_TEMP" "Net-(J1-Pad2)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class ChargerCurrent
    "/CHARGER_OUT+" "/CHARGER_OUT-" /CHARGER+ "/CHARGER-" "/CHARGER_OUT+_FUSED"
    (clearance_class ChargerCurrent)
    (via_rule ChargerCurrent)
    (rule
      (width 4000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class PowerCurrent
    "/12V_SRC" +12V GND "/AIRS_CHARGER_OUT"
    (clearance_class PowerCurrent)
    (via_rule PowerCurrent)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)